{
  "root": "src",
  "pageDir": "page",
  "outDir": "dist",
  "assets": [
    "assets",
    "favicon.ico"
  ],
  "buildOptions": {
    "progress": true,
    "showCircularDependencies": true,
    "outputHashing": "none",
    "deployUrl": "",
    "extractCss": true,
    "sourcemaps": true,
    "verbose": false,
    "target": "production",
    "deleteOutputPath": true,
    "base64Limit": 4000
  },
  "pageOptions": {
    "defaultName": "index",
    "defaultTitle": "Untitled",
    "defaultTemplate": "",
    "inlineJs": false,
    "inlineCss": false
  },
  "serveOptions": {
    "host": "0.0.0.0",
    "port": 80
  },
  "debug": {
    "inject": []
  },
  "pageConfig": "page-config.json"
}